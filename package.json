{
  "name": "adonisjs-infobip",
  "version": "1.0.1",
  "description": "An addon/plugin package to provide InfoBip single/bulk SMS/Voice services in AdonisJS 4.0+",
  "main": "index.js",
  "files": [
    "src",
    "config",
    "providers",
    "instructions.js",
    "instructions.md"
  ],
  "directories": {
    "test": "test"
  },
  "scripts": {
    "lint": "standard",
    "pretest": "npm run lint",
    "test": "nyc node japaFile.js",
    "test:win": "node ./node_modules/japa-cli/index.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "keywords": [
    "sms",
    "tts",
    "voice",
    "rest-api",
    "infobip",
    "adonis-addon",
    "adonisjs",
    "adonis-framework"
  ],
  "nyc": {
    "exclude": [
      "test"
    ]
  },
  "standard": {
    "globals": [
      "use",
      "test"
    ]
  },
  "author": "StitchNG <stitchnig@gmail.com> (https://twitter.com/stitchappn)",
  "license": "MIT",
  "dependencies": {
    "@adonisjs/generic-exceptions": "^2.0.1",
    "infobip-nodejs": "^0.1.0"
  },
  "devDependencies": {
    "japa": "^2.0.5",
    "japa-cli": "^1.0.1",
    "nyc": "^13.0.1",
    "semver": "^5.5.0",
    "standard": "^12.0.1",
    "@adonisjs/sink": "^1.0.6",
    "@adonisjs/fold": "^4.0.9"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stitchng/adonis-infobip.git"
  },
  "bugs": {
    "url": "https://github.com/stitchng/adonis-infobip/issues"
  },
  "homepage": "https://github.com/stitchng/adonis-infobip"
}
